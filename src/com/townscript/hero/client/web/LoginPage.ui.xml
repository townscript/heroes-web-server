<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		
	</ui:style>

<g:HTMLPanel>
	<g:FlowPanel>
		<g:Label text="User Name"></g:Label>
		<g:TextBox ui:field="userName"></g:TextBox>
		
		<g:Label text="Password"></g:Label>
		<g:PasswordTextBox ui:field="password"></g:PasswordTextBox>
	
	</g:FlowPanel>
	<g:FlowPanel>
		<g:Button ui:field="login" text="Sign in"></g:Button>
		<g:Label ui:field="signup" styleName="clickstyle" text="Click here to Register"></g:Label>
	</g:FlowPanel>
</g:HTMLPanel>
</ui:UiBinder> 