<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:b="urn:import:org.gwtbootstrap3.client.ui"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		.onclickstyle{
	
	
		}
	</ui:style>
	
	<b:Container styleName="container-fluid">


		<b:Row>
			<b:Column size="MD_4" offset="MD_4">
				<b:Div styleName="user-box">

					<b:InputGroup styleName="form-group input-group">
						<b:InputGroupAddon icon="USER"></b:InputGroupAddon>
						<b:TextBox ui:field="firstname" placeholder="First Name" />
					</b:InputGroup>
					
					<b:InputGroup styleName="form-group input-group">
						<b:InputGroupAddon icon="USER"></b:InputGroupAddon>
						<b:TextBox ui:field="lastname" placeholder="Last Name" />
					</b:InputGroup>
					
					<b:InputGroup styleName="form-group input-group">
						<b:InputGroupAddon icon="USER"></b:InputGroupAddon>
						<b:TextBox ui:field="email"
							placeholder="Email ID" />
					</b:InputGroup>

					<b:InputGroup styleName="form-group input-group">
						<b:InputGroupAddon icon="LOCK"></b:InputGroupAddon>
						<b:PasswordTextBox ui:field="password"
							placeholder="Password" />
					</b:InputGroup>

					<b:Div styleName="form-group text-center">
						<b:Button type="PRIMARY" ui:field="signup" text="Sign Up"></b:Button>
					</b:Div>
					<b:Div styleName="text-center">
						<b:Anchor styleName="text-primary" href="#login"
							text="Already user?Click here to login"></b:Anchor>
					</b:Div>

				</b:Div>
			</b:Column>
		</b:Row>

	</b:Container>
</ui:UiBinder> 